#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
struct student{
    int members;
    char name[10];
    int age;
    int  totalmarks;
};

int main() {
    struct student s[100];
    int i,n;
    //int members;
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        scanf("%s",s[i].name);
       scanf("%d",&s[i].age);
     scanf("%d",&s[i].totalmarks);
    }
    for(i=1;i<=n;i++){
        printf("Name: %s\n",s[i].name);
         printf("Age: %d\n",s[i].age);
          printf("Total Marks: %d\n",s[i].totalmarks);
          printf("\n");
    }
    
    
    
    return 0;
}

int main(){
    int i,j=0,k=0,n;
    scanf("%d",&n);
    int a[100];
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int b[100],c[100];
    for(i=0;i<n;i++)
    {
        if(a[i]==0)
        {
            b[k]=i;
            k++;
        }
        else if (a[i]==1) 
        {
        c[j]=i;
        j++;
        }
    }
        for(i=0;i<k;i++){
            printf("%d ",a[b[i]]);
        }
        for(i=0;i<j;i++){
            printf("%d ",a[c[i]]);
        }
    return 0;
}
